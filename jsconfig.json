{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "maxNodeModuleJsDepth": 0
  },
  "exclude": [
    "node_modules"
  ]
}
